# Environment variables for production deployment
# Copy this file to .env on your VPS at /opt/szybkie-wyzwania/.env

# Database Configuration
DB_NAME=szybkie_wyzwania_prod
DB_USER=szybkie_wyzwania_user
DB_PASSWORD=CHANGE_THIS_TO_SECURE_PASSWORD

# Django Configuration
SECRET_KEY=GENERATE_RANDOM_SECRET_KEY_MIN_50_CHARS
ALLOWED_HOSTS=szybkie-wyzwania.pl,www.szybkie-wyzwania.pl
DEBUG=False

# GitHub Container Registry
GITHUB_USERNAME=your-github-username

# Generate SECRET_KEY with:
# python3 -c 'from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())'
